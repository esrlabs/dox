document: verification_method_test

ID_1:
  text: >
        Dim file should allow only verification_methods attribute,
        but when test_setups method is called on the requirement object
        it shall return the array that verification_methods attribute
        mentions in the Dim file

ID_2:
  verification_methods: off_target, on_target

ID_3:
  verification_methods: off_target,off_target

ID_4:
  verification_methods:       |
                                off_target,on_target,off_target,on_target

ID_5:
  test_setups: off_target, on_target, manual
